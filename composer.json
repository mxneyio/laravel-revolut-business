{
    "name": "mxneyio/laravel-revolut-business",
    "description": "An unofficial Laravel wrapper for Revolut's Open API for Business",
    "license": "MIT",
    "require": {
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "tbclla\\Revolut\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tbclla\\Revolut\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "tbclla\\Revolut\\Providers\\RevolutServiceProvider"
            ],
            "aliases": {
                "Revolut": "tbclla\\Revolut\\Facades\\Revolut"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    }
}
